{
    "log": {
        "loglevel": "info"
    },
    "dns": {
        "servers": [
            "https+local://8.8.8.8/dns-query"
        ]
    },
    "inbounds": [
        {
            "port": 8080,
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "d54c2a77-3836-421e-b5bd-dbeeda852ab7"
                    }
                ],
                "decryption": "none" 
            },
            "streamSettings": {
                "network": "ws",
                "wsSettings": {
                    "path": "qwe"
                }
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls",
                    "quic"
                ]
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "tag": "direct",
            "settings": {
                "domainStrategy": "UseIPv4"
            }
        }
    ]
}
